{
  "name": "neo-school-admin-portal",
  "version": "0.1.0",
  "author": "Neo Financial Engineering <engineering@neofinancial.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=22.2.0",
    "npm": ">=10.9.2"
  },
  "scripts": {
    "build": "next build",
    "codegen": "npm run codegen:clear && graphql-codegen --config codegen.yaml",
    "codegen:clear": "rimraf **/__graphql",
    "codegen:watch": "npm run codegen -- --watch",
    "download:schema": "graphql-codegen --config download.yaml",
    "format": "prettier --write --log-level warn \"**/*.{ts,tsx,js,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,graphql,gql,yaml,yml,md}\"",
    "lint": "next lint",
    "precommit": "npm run format && eslint --fix --max-warnings 0 && git add -u",
    "start": "npm run download:schema && node config/index && npm run codegen && concurrently -n portal,codegen -c bgMagenta.white,bgMagenta.black \"next dev\" \"npm run codegen:watch\"",
    "start:next": "next start",
    "test": "TZ=UTC jest --maxWorkers=3",
    "test:changed": "npm test -- -- --changedSince=master",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "typecheck": "graphql-codegen --config codegen.yaml && tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.8.9",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.4.2",
    "@mui/material": "^5.15.4",
    "@mui/styles": "^5.15.4",
    "@mui/x-date-pickers": "^6.19.0",
    "@neofinancial/morpheus-components": "^17.1.2",
    "@neofinancial/morpheus-icons": "^0.8.1",
    "@neofinancial/morpheus-illustrations": "^0.4.0",
    "@neofinancial/morpheus-theme-light": "^0.9.2",
    "@neofinancial/neo-configcat": "^0.3.0",
    "bson-objectid": "^2.0.4",
    "configcat-common": "^8.1.1",
    "configcat-js-ssr": "^7.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "isomorphic-unfetch": "^4.0.0",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "notistack": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.6",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-tools/mock": "^9.0.0",
    "@neofinancial/eslint-plugin-web-linting": "^0.6.4",
    "@neofinancial/morpheus-mui-overrides": "^0.7.0",
    "@neofinancial/neo-test-factory": "^1.1.0",
    "@neofinancial/web-linting": "^0.1.1",
    "@neofinancial/web-tsconfig": "^0.1.1",
    "@parcel/watcher": "^2.3.0",
    "@swc/core": "^1.5.27",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fail-on-console": "^3.1.1",
    "msw": "^1.2.5",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.1",
    "typescript": "^4.7.4"
  }
}
